<Window x:Class="CB.LogsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CB"
        mc:Ignorable="d"
        Title="LogsWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Back to Admin Window" Click="btnBackToAdmin_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"/>
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
            <TextBox x:Name="txtLogs" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="10,40,10,10" TextWrapping="Wrap" AcceptsReturn="True" IsReadOnly="True"/>
        </ScrollViewer>
    </Grid>
</Window>
